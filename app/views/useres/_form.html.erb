<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :last_name %>
    <%= f.input :nick %>
    <%= f.input :password %>
    <%= f.input :phone %>
    <%= f.input :identification_number %>
    <%= f.input :emai %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
