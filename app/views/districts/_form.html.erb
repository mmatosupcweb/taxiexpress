<%= simple_form_for @district do |f| %>
  <%= f.input :name %>
  <%= error_span(@district[:name]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                districts_path, :class => 'btn btn-default' %>
<% end %>
